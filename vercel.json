{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/webhook",
      "dest": "app.py"
    },
    {
      "src": "/",
      "dest": "app.py"
    }
  ],
  "env": [
    "TELEGRAM_BOT_TOKEN",
    "OPENAI_API_KEY",
    "VT_API_KEY",
    "AZURE_API_KEY"
  ]
}